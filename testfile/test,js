const test = () => {
  console.log("hello");
};
// 익명 함수 표현식
let add = function (x, y) {
  return x + y;
};

let sum = add;
// 기명 함수 표현식
let sum1 = function add(x, y) {
  return x + y;
};

console.log(add(1, 1));

console.log(sum1(2, 2));

console.log(sum(1, 4));

const fact = (x) => {
  answer = 1;
  for (let i = x; i > 0; i--) {
    answer *= i;
  }
  return answer;
};
console.log(fact(4));

abc = [1, 2, 3, 4, 5];

for (i in abc) {
  console.log(fact(i));
}

